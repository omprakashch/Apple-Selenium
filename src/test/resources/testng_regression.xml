<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Apple Application Test Automation Suite" verbose="1" parallel = "methods" thread-count="1" >

	 <listeners>
		<listener class-name="com.apple.qa.listeners.ExtentReportListener" />
	</listeners>


	<test name="Apple App Test Cases">
		<classes>
			<class name="com.apple.qa.testcases.AccessoriesPageTest"/>
			<class name="com.apple.qa.testcases.AirPodsPageTest"/>
			<class name="com.apple.qa.testcases.BuyIPhoneAppleStorePageTest"/>
			<class name="com.apple.qa.testcases.CartPageTest"/>
			<class name="com.apple.qa.testcases.HomePageTest"/>
			<class name="com.apple.qa.testcases.IPadPageTest"/>
			<class name="com.apple.qa.testcases.IPhonePageTest"/>
			<class name="com.apple.qa.testcases.MacPageTest"/>
			<class name="com.apple.qa.testcases.StorePageTest"/>
			<class name="com.apple.qa.testcases.SupportPageTest"/>
			<class name="com.apple.qa.testcases.TVAndHomePageTest"/>
			<class name="com.apple.qa.testcases.VisionPageTest"/>
			<class name="com.apple.qa.testcases.WatchPageTest"/>	
		</classes>
	</test>
</suite>